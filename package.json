{
  "name": "umble",
  "version": "1.0.0",
  "description": "Humble Pulumi Constructs for every app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lepozepo/umble"
  },
  "scripts": {
    "pack:build": "pack build",
    "pack:publish": "pack publish",
    "test": "npx npm-run-all --parallel test:*",
    "test:lint": "eslint ./src --quiet -c ./.eslintrc.js",
    "test:unit": "jest --ci --passWithNoTests",
    "version": "npm run pack:build"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "**/*.test.js"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "dependencies": {
    "@pulumi/aws": "^1.16.0",
    "@pulumi/awsx": "^0.18.14",
    "@pulumi/pulumi": "^1.8.1",
    "@pulumi/tls": "^1.4.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.8.1",
    "@pika/plugin-standard-pkg": "^0.8.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "jest": "^24.9.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
